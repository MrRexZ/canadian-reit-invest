{
  "name": "canadian-reit-invest",
  "scripts": {
    "anchor": "cd anchor && anchor",
    "anchor-build": "cd anchor && anchor build",
    "anchor-localnet": "cd anchor && solana-test-validator --clone-upgradeable-program metaqbxxUerdq28cj1RbAWkYQm3ybzjb6a8bt518x1s --url https://api.mainnet-beta.solana.com --ledger ./ledger",
    "anchor-test": "cd anchor && anchor test",
    "build": "tsc -b && vite build --mode production",
    "codama:js": "codama run js -c codama.js",
    "ci": "npm run build && npm run lint && npm run format:check && npm run codama:js",
    "local": "vite",
    "dev": "vite --mode development",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "preview": "vite preview",
    "prod": "npm run build && vite preview --port 4173",
    "setup": "npm run anchor keys sync && npm run codama:js",
    "deploy:devnet": "cd anchor && anchor build && anchor deploy --provider.cluster devnet",
    "deploy:localnet": "cd anchor && anchor build && anchor deploy --provider.cluster localnet"
  },
  "description": "React+Vite, Tailwind, gill (based on @solana/kit), canadianreitinvest Anchor example, Wallet UI",
  "keywords": [
    "anchor-canadianreitinvest",
    "gill",
    "react",
    "solana-kit",
    "tailwind",
    "typescript",
    "vite",
    "wallet-ui"
  ],
